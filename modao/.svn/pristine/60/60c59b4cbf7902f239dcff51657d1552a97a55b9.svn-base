package nuanliu.com.modao.bean;

public class RepairScheduleBean {

    private int state;
    private String mhead;
    private String mdate;
    private String mcontent;

    public RepairScheduleBean(String mhead, String mdate, String mcontent) {
        this.mhead = mhead;
        this.mdate = mdate;
        this.mcontent = mcontent;
    }

    public int getState() {
        return state;
    }

    public void setState(int state) {
        this.state = state;
    }

    public String getMhead() {
        return mhead;
    }

    public void setMhead(String mhead) {
        this.mhead = mhead;
    }

    public String getMdate() {
        return mdate;
    }

    public void setMdate(String mdate) {
        this.mdate = mdate;
    }

    public String getMcontent() {
        return mcontent;
    }

    public void setMcontent(String mcontent) {
        this.mcontent = mcontent;
    }

}
